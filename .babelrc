						"targets": {
							"node": "current"
						}
					}
				]
			],
			"plugins": [
				"transform-runtime",
				[
					"styled-components",
					{ "ssr": true, "displayName": true, "preprocess": false }
				]
			]
		},
		"production": {
			"presets": "next/babel"
		},
		"1": {
			"presets": ["next/babel"],
			"plugins": [
				[
					"styled-components",
					{ "ssr": true, "displayName": true, "preprocess": false }
				]
			]
		}
	}
}
